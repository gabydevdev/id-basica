{
	"name": "id-basica",
	"version": "1.0.0",
	"scripts": {
		"scss:lint": "wp-scripts lint-style",
		"scss:fix": "wp-scripts lint-style --fix",
		"style:build": "sass src/scss/main.scss:build/css/main.css --style=expanded",
		"style:watch": "sass src/scss/main.scss:build/css/main.css --watch --style=expanded",
		"php:lint": "composer run-script lint",
		"php:format": "composer run-script format",
		"start": "set NODE_ENV=development && wp-scripts start",
		"build": "wp-scripts build"
	},
	"devDependencies": {
		"@wordpress/browserslist-config": "^6.26.0",
		"@wordpress/scripts": "^30.19.0",
		"@wordpress/stylelint-config": "^23.18.0",
		"autoprefixer": "^10.4.21",
		"browserslist": "^4.25.1",
		"path": "^0.12.7",
		"postcss": "^8.5.6",
		"postcss-cli": "^11.0.1",
		"prettier": "^3.6.2",
		"sass": "^1.89.2",
		"stylelint": "^16.21.1",
		"webpack-remove-empty-scripts": "^1.1.1"
	},
	"dependencies": {
		"sprucecss": "^2.3.4"
	}
}
